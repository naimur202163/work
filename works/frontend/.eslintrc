{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "plugins": ["import"],
    "extends": [
        "react-app",
        "plugin:prettier/recommended"
      ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "off",
      "import/first": "error",
      "react-hooks/exhaustive-deps": "off", 
      "no-unused-vars": "error"
    }
  }